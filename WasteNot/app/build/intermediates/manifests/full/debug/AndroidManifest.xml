<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.wastenot.wastenot"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.wastenot.wastenot.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.wastenot.wastenot.HaveListActivity"
            android:label="@string/title_activity_have_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="br.com.wastenot.wastenot.MyDeckActivity"
            android:label="@string/title_activity_my_deck"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="br.com.wastenot.wastenot.CardDetail" />
        <activity android:name="br.com.wastenot.wastenot.SettingActivity" />
    </application>

</manifest>